<?xml version="1.0" encoding="UTF-8"?>

<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

<servlet>

<servlet-name>MyServer</servlet-name>

<servlet-class>com.DianaGolik.MyServer</servlet-class>

</servlet>

<servlet>

<servlet-name>RegForm</servlet-name>

<servlet-class>com.DianaGolik.RegForm</servlet-class>

</servlet>
<servlet-mapping>

<servlet-name>MyServer</servlet-name>

<url-pattern>/MyServer</url-pattern>

</servlet-mapping>

<servlet-mapping>

<servlet-name>RegForm</servlet-name>

<url-pattern>/RegForm</url-pattern>

</servlet-mapping>

<session-config>

<session-timeout>

30

</session-timeout>

</session-config>

<welcome-file-list>

<welcome-file>MyServlet</welcome-file>

</welcome-file-list>

</web-app>